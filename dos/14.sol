pragma solidity >= 0.4.19 < 0.6.0;

contract Test1 {
    uint256 public goal = 5000;

    function getGoal() public view returns (uint256) {
        return goal;
    }

    function setGoal(uint256 x) public {
        goal = x;
    }
    
}

contract Test2 {

    address[] members;
    uint256 winner = 0xffff;

    function addMember(address addr) public {
        members.push(addr);
    }

    function chooseWinner(Test1 t1) public {
        uint256 result;
        while (result >= members.length) {
            result = block.timestamp % (t1.getGoal());
        } // 生成满足的result可能要迭代很多次
        transferToWinner(1);
    }

    function transferToWinner(uint amount) public {
        require(winner != 0xffff);
        address(members[winner]).transfer(amount);
    }



}
