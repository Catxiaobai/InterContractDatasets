pragma solidity >= 0.4.19 < 0.6.0;

contract Test1 {
    uint256 public goal = 0;

    function getGoal() public view returns (uint256) {
        return goal;
    }

    function setGoal(int seed) public {
        uint256 tmp = 0;
        while (seed != 0) { // seed < 0 可能无限循环
            seed -= 1;
            tmp += block.timestamp;
            tmp %= block.number;
        }
        goal = tmp;
    }
    
}

contract Test2 {

    address[] members;
    uint256 winner = 0xffff;

    function addMember(address addr) public {
        members.push(addr);
    }



    function bet(Test1 t1) public {
        transferToWinner(members[t1.getGoal() % members.length], 1);
    }

    function transferToWinner(address addr, uint amount) public {
        address(addr).transfer(amount);
    }



}
