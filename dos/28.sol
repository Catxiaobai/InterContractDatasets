pragma solidity >= 0.4.19 < 0.6.0;

contract Test1 {
    uint256 public goal = 5000;

    function getGoal() public view returns (uint256) {
        return goal;
    }

    function setGoal(uint256 x) public {
        goal = x;
    }
    
    function transferMoney(address addr, uint amount) public {
        addr.transfer(amount);
    }
}

contract Test2 {

    function loopCall(Test1 t1, address addr) public {
        while (t1.getGoal() != 0) {
            t1.transferMoney(addr, t1.getGoal());
        }
    }

    
}
