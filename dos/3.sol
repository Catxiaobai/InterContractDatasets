contract Test1 {
    uint256 public goal = 5000;

    function getGoal() public view returns (uint256) {
        return goal;
    }

    function sendGoal(address addr) public {
        addr.transfer(1 ether);
    }
}

contract Test2 {
    function transferMoney(Test1 t1, address addr) public {
        while (t1.getGoal() > 0) {
            t1.sendGoal(addr);
        }
    }
}
