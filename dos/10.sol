pragma solidity >= 0.4.19 < 0.6.0;

contract Test1 {
    uint256 public goal = 5000;

    function getGoal() public view returns (uint256) {
        return goal;
    }

    function setGoal(uint256 x) public {
        goal = x;
    }
}

contract Test2 {

    function transferMoney(Test1 t1) public {
        while (true) {
            uint256 goal = t1.getGoal();
            if (goal > 100) {
                goal -= 100;
                t1.setGoal(goal);
            }
            else break;
        }
    }
}
