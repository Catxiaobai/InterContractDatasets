pragma solidity >= 0.4.19 < 0.6.0;

contract Test1 {
    uint256 public goal = 5000;

    function getGoal() public view returns (uint256) {
        return goal;
    }

    function setGoal(uint256 x) public {
        goal = x;
    }

    function transferMoney(address addr) public {
        addr.transfer(1);
        goal -= 1;

    }
    
}

contract Test2 {
    address[] members;
    mapping(address => uint256) wallet;
    
    function transferMoney(Test1 t1, address addr) public {
        while (true) {
            uint256 goal = t1.getGoal();
            if (goal >= 1) {
                t1.transferMoney(addr);
            }
        }
        

    }
}
